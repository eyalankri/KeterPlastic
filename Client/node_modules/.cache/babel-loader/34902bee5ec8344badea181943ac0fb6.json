 @click=\"selectProduct(prod.id)\">remove</v-btn>              \r\n                <v-spacer></v-spacer>\r\n                <v-btn icon @click=\"show = !show\">\r\n                  <v-icon>{{ show ? 'mdi-chevron-up' : 'mdi-chevron-down' }}</v-icon>\r\n                </v-btn>\r\n              </v-card-actions>\r\n\r\n              <v-expand-transition>\r\n                <div v-show=\"show\">\r\n                  <v-divider></v-divider>\r\n                  <v-card-text>{{prod.description}}</v-card-text>\r\n                </div>\r\n              </v-expand-transition>\r\n            </v-card>\r\n          </v-col>\r\n        </div>\r\n      </v-row>\r\n    </v-container>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport moment from \"moment\";\r\n\r\nexport default {\r\n  data: () => ({\r\n    show: false,\r\n    products: null\r\n  }),\r\n  methods: {\r\n    selectProduct(id) {\r\n      this.$store.dispatch(\"selectProduct\", id);\r\n    },   \r\n  },\r\n  mounted() {\r\n    this.products = this.$store.state.products;\r\n    this.products.forEach(item => {\r\n            \r\n      item.estDelieryDate = moment()\r\n        .add(item.estDelieryDays, \"days\")\r\n        .format(\"DD/MM/YYYY\");\r\n    });\r\n\r\n    this.products = this.products.sort(\r\n      (a, b) =>  new Date(a.estDelieryDays) - new Date(b.estDelieryDays) \r\n    );\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}