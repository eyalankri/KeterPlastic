t(id) {\r\n      this.$store.dispatch(\"selectProduct\", id);\r\n    },\r\n    getRates() {\r\n      axios\r\n        .get(\"https://api.exchangeratesapi.io/latest?symbols=USD,ILS\")\r\n        .then(res => {\r\n          // eslint-disable-next-line\r\n          console.log(res.data);\r\n          /*\r\n          this.city = null;\r\n          this.street = null;\r\n          this.buildingNumber = null;\r\n          this.projectName = null;\r\n          this.feedback = \"נשמר בהצלחה!\";\r\n          this.progressBar = null;\r\n          var table = $(\"#buildings\").DataTable();\r\n          table.destroy(); // must destroy before calling again\r\n          this.listBuildings();\r\n          */\r\n        })\r\n        // eslint-disable-next-line\r\n        .catch(error => {\r\n          //  this.$router.push({\r\n          //  name: \"Login\"\r\n          //  });\r\n        });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.getRates();\r\n    this.products = this.$store.state.products;\r\n    this.products.forEach(item => {\r\n      item.estDelieryDate = moment()\r\n        .add(item.estDelieryDays, \"days\")\r\n        .format(\"DD/MM/YYYY\");\r\n    });\r\n\r\n    this.products = this.products.sort(\r\n      (a, b) => new Date(a.estDelieryDays) - new Date(b.estDelieryDays)\r\n    );\r\n  }\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}